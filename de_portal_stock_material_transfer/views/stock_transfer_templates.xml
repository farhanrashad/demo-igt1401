<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- -->
    <template id="portal_layout" name="Portal layout: material menu entry" inherit_id="portal.portal_breadcrumbs" priority="40">
        <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
            <li t-if="page_name == 'stock_material' or stock_material" t-attf-class="breadcrumb-item #{'active ' if not stock_material else ''}">
                <a t-if="stock_material" t-attf-href="/stock/materials?{{ keep_query() }}">Material Requisition</a>
                <t t-else="">Material Requisition</t>
            </li>
        </xpath>
    </template>
    <!-- -->
    <template id="portal_my_home" name="Show Hoto / Tasks" customize_show="True" inherit_id="portal.portal_my_home" priority="40">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Material Requisition</t>
                <t t-set="url" t-value="'/stock/materials'"/>
                <t t-set="placeholder_count" t-value="'material_count'"/></t>
        </xpath>
    </template>
    <!-- -->
    <template id="portal_stock_materials" name="Stock Materials">
        <t t-call="portal.portal_layout">
            <t t-set="breadcrumbs_searchbar" t-value="True"/>
            <t t-call="portal.portal_searchbar">
                <t t-set="title">Material Requisition</t>
            </t>
            <t t-if="not grouped_materials">
                <div class="alert alert-warning mt8" role="alert">
                    There are no Material Requisition.
                </div>
            </t>
            <t t-if="grouped_materials">
                <t t-call="portal.portal_table">
                    <t t-foreach="grouped_materials" t-as="materials">
                        <thead>
                            <tr t-attf-class="{{'thead-light' if not groupby == 'none' else ''}}">
                                <th class="text-left">Ref</th>
                                <th >Order Date</th>
                                <th >Date Scheduled</th>
                                <th >Contractor</th>
                                <th >Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="materials" t-as="material">
                                <tr>
                                    <td class="text-left">
                                        #
                                        <a t-attf-href="/stock/material/#{material.id}?{{ keep_query() }}">
                                        <span t-esc="material.name"/>
                                        </a>
                                    </td>
                                    <td>
                                            <span t-field="material.date_order" t-options='{"widget": "date"}'/>
                                    </td>
                                    <td class="text-left">
                                            <span t-field="material.date_order" t-options='{"widget": "date"}'/>
                                    </td>
                                    <td class="text-left">
                                        <span t-esc="material.partner_id.name"/>
                                    </td>

                                    <td class="text-left">
                                        <span t-esc="material.stage_id.name"/>
                                    </td>
                                </tr>
                            </t>
                            <tr>
                                <div>
                                    <t class="col-lg-7 col-md-8" >
                                        <a href="/stock/material/type/" >
                                            <div class="btn btn-primary pull-left" style="height:30px; line-height:100%; ">Create</div>
                                        </a>
                                    </t>
                                </div>
                            </tr>
                        </tbody>
                    </t>
                </t>
            </t>
        </t>
    </template>
    <!-- -->
    <template id="portal_stock_material" name="Stock Material">
        <t t-call="portal.portal_layout">
            <t t-call="portal.portal_record_layout">
                <t t-set="card_header">
                    <div class="row no-gutters">
                        <div class="col-12">
                            <h5 class="d-flex mb-1 mb-md-0 row">
                                <span t-field="stock_material.name" class="col-9 text-truncate"/>
                                <small class="text-muted d-none d-md-inline"> (#
                                    <span t-field="stock_material.id"/>)
                                
                                </small>
                                <div class="col-3 col-md-2 text-right">
                                    <small class="text-right">Status:</small>
                                    <span t-field="stock_material.stage_id.name" class=" badge badge-pill badge-info" title="Current stage of this material s"/>
                                </div>
                            </h5>
                        </div>
                    </div>
                </t>
                <t t-set="card_body">

                    <div class="row mb-4">
                        <div class="col-12 col-md-6 mb-1">
                            <strong>Order Date:</strong>
                            <span t-field="stock_material.date_order" t-options='{"widget": "date"}'/>
                        </div>
                        <div class="col-12 col-md-6" >
                            <strong>Date Scheduled:</strong>
                            <span t-field="stock_material.date_scheduled"  t-options='{"widget": "date"}' />
                        </div>
                    </div>
                    <div class="row mb-4">
                        <div class="col-12 col-md-6 mb-1">
                             <strong>Contractor:</strong><span t-field="stock_material.partner_id.name" />
                        </div>
                        <div class="col-12 col-md-6" >
                            <strong>Transfer Type:</strong>

                            <span t-field="stock_material.transfer_order_type_id.name"  />
                        </div>
                    </div>
                    <div class="row mb-4">
                        <div class="col-12 col-md-6 mb-1">
                            <strong>Category:</strong>
                            <span t-field="stock_material.transfer_order_category_id.name"  />
                        </div>
                    </div>
                    <h4><p style="color:blue;">Product Section</p></h4>
                     <table class="table table-sm table-striped table-bordered" id="stockTable">


                            <tr t-attf-class="{{'thead-light' if not groupby == 'none' else ''}}">

                                <th class="text-left">Product</th>
                                <th class="text-left">Description</th>
                                <th class="text-left">Quantity</th>
                                <th class="text-left">Project</th>
                                <th class="text-left">Schedule Date</th>
                                <th class="text-left">Supplier</th>
                            </tr>
                        <t t-foreach="stock_material.stock_transfer_order_line" t-as="line">
                               <tr>
                                     <td class="product">

                                        <span t-esc="line.product_id.name" />
                                    </td>
                                   <td class="description">

                                        <span t-esc="line.name" />
                                    </td>
                                  <td class="quantity">

                                        <span t-esc="line.product_uom_qty" />
                                    </td>
                                <td class="project">

                                        <span t-esc="line.project_id.name" />
                                    </td>
                            <td class="sechdule">

                                        <span t-esc="line.date_scheduled" />
                                    </td>
                            <td class="supplier">

                                        <span t-esc="line.supplier_id.name" />
                                    </td>
                               </tr>

                        </t>
                     </table>
                    <t t-if="stock_material.stock_transfer_return_line">
                    <h4><p style="color:blue;">Returnable Section</p></h4>
                     <table class="table table-sm table-striped table-bordered" id="stockTable">


                            <tr t-attf-class="{{'thead-light' if not groupby == 'none' else ''}}">

                                <th class="text-left">Product</th>
                                <th class="text-left">Description</th>
                                <th class="text-left">Quantity</th>
                                <th class="text-left">Project</th>
                                <th class="text-left">Schedule Date</th>
                            </tr>
                        <t t-foreach="stock_material.stock_transfer_return_line" t-as="line">
                               <tr>
                                     <td class="product">

                                        <span t-esc="line.product_id.name" />
                                    </td>
                                   <td class="description">

                                        <span t-esc="line.name" />
                                    </td>
                                  <td class="quantity">

                                        <span t-esc="line.product_uom_qty" />
                                    </td>
                                <td class="project">

                                        <span t-esc="line.project_id.name" />
                                    </td>
                            <td class="sechdule">

                                        <span t-esc="line.date_scheduled" />
                                    </td>

                               </tr>

                        </t>
                     </table>
                   </t>
                </t>
            </t>
            <div class="mt32">
                <h4>
                    <strong>Message and communication history</strong>
                </h4>
                <t t-call="portal.message_thread">
                    <t t-set="object" t-value="stock_material"/>
                    <t t-set="pid" t-value="pid"/>
                    <t t-set="hash" t-value="hash"/></t>
            </div>
        </t>
    </template>
    <!-- Request Submission Form -->
    <template id="de_portal_stock_material_transfer.stock_material_submited" name="stock Received">
        <t t-call="portal.portal_layout">
            <div id="wrap" class="oe_structure oe_empty">
                <div class="container">

                    <div class="row">
                        <h1 class="text-center">
                            <i class="fa fa-check-circle fa-2x text-success" role="img" aria-label="Success" title="Success"/>
                        </h1>
                        <h1 class="text-center">Thank you for submitting your request!</h1>
                    </div>
                </div>
            </div>
        </t>
    </template>
